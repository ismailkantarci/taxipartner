{
  "name": "taxipartner-admin-suite",
  "version": "1.3.1100",
  "private": true,
  "type": "module",
  "scripts": {
    "codespaces:setup": "bash Codespaces/01_bootstrap.sh",
    "codespaces:start": "npm-run-all --parallel codespaces:identity codespaces:admin codespaces:spa codespaces:taxipartner-admin",
    "codespaces:identity": "npm --prefix identity run dev",
    "codespaces:admin": "npm run dev",
    "codespaces:spa": "npm --prefix frontend run dev",
    "codespaces:taxipartner-admin": "npm --prefix taxipartner-admin run dev -- --host --port 5179",
    "dev:openai:test": "node -e 'import(\"dotenv/config\");import OpenAI from \"openai\";const c=new OpenAI({apiKey:process.env.OPENAI_API_KEY});(async()=>{const r=await c.chat.completions.create({model:process.env.OPENAI_MODEL||\"gpt-5-pro\",messages:[{role:\"user\",content:\"Return: ok\"}]});console.log(r.choices?.[0]?.message?.content||\"no result\")})().catch(e=>{console.error(e?.response?.data||e);process.exit(1);});'",
    "identity:test": "npx tsx identity/tests/roleGuard.spec.ts && npx tsx identity/tests/permissionTemplates.spec.ts",
    "test:identity": "npm run identity:test",
    "predev": "node scripts/strip-webkit-text-size-adjust.mjs || true",
    "dev": "npm-run-all --parallel dev:css dev:vite",
    "dev:vite": "vite",
    "dev:css": "tailwindcss -i src/styles/tailwind.css -o output.css --watch --postcss",
    "verify:mp18": "node --loader tsx scripts/verify.mp18.mjs",
    "build": "vite build && tailwindcss -i src/styles/tailwind.css -o dist/output.css --minify && node scripts/strip-webkit-text-size-adjust.mjs || true",
    "preview": "vite preview",
    "build:css": "tailwindcss -i src/styles/tailwind.css -o dist/output.css --minify",
    "test": "vitest run",
    "test:ui": "vitest",
    "test:e2e": "playwright test --reporter=line",
    "preview:staging": "vite preview --port 4173",
    "lhci": "lighthouse http://localhost:4173 --quiet --chrome-flags='--headless=new' --only-categories=performance,accessibility,best-practices,seo",
    "sandbox": "node scripts/sandbox-server.mjs",
    "library:demo": "npx http-server modules/library/docs/demo -p 4173 -c-1",
    "library:build": "npm --prefix modules/library run build",
    "library:test": "vitest run modules/library/components/phone-input/phone-input.test.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "version:sync": "node scripts/version-sync.mjs",
    "auto-release:setup": "node scripts/auto-release-setup.mjs",
    "watch:auto-release": "bash scripts/local_watch.sh start",
    "watch:auto-release:stop": "bash scripts/local_watch.sh stop",
    "watch:auto-release:status": "bash scripts/local_watch.sh status",
    "changelog": "node scripts/generate-changelog.mjs",
    "validate:modules": "node scripts/validate-modules.mjs",
    "validate:releases": "node scripts/validate-release-log.mjs",
    "sbom": "cyclonedx-npm --json -o release-pack/sbom.json",
    "license:report": "license-checker-rseidelsohn --json --production --out release-pack/licenses.json",
    "release:pack": "node scripts/build-release-pack.mjs",
    "release:auto": "node scripts/auto-release-log.mjs",
    "prepare": "husky install",
    "postinstall": "node scripts/auto-release-setup.mjs || true",
    "typecheck": "tsc -p tsconfig.typecheck.json",
    "check": "npm run typecheck",
    "build:qa": "npm run check && vite build",
    "qa:summary": "node --loader tsx scripts/route_map.mjs",
    "identity:dev": "npx tsx identity/src/server.ts",
    "identity:build": "tsc -p identity/tsconfig.json",
    "scheduler:dev": "npx tsx identity/src/tasks.scheduler.ts",
    "import:registers": "npm --prefix identity run import:registers --",
    "db:up": "docker compose up -d",
    "db:down": "docker compose down",
    "db:gen": "prisma generate",
    "db:migrate": "prisma migrate dev --name migrate_pg",
    "db:seed": "tsx prisma/seed.ts",
    "db:ensure": "prisma generate && prisma migrate dev --name mp18_sync",
    "seed:mp18": "tsx scripts/seed.mp18.ts",
    "db:backup": "node scripts/pg-backup.js",
    "db:restore": "node scripts/pg-restore.js",
    "tasks:smoke": "npx tsx identity/scripts/task.smoke.ts",
    "ai:sample": "node scripts/ai/sample-openai.js \"Merhaba, test\"",
    "ai:review": "node scripts/ai/sample-openai.js \"Bu diff i√ßin kod inceleme yorumu ver\" --diff /tmp/diff.patch",
    "lighthouse": "npx -y lighthouse http://127.0.0.1:8080 --only-categories=performance,accessibility,best-practices,seo --quiet --chrome-flags='--headless=new'",
    "lighthouse:report": "npm run lighthouse -- --output html --output-path docs/lighthouse/rapor-$(date +%Y%m%d).html",
    "verify": "npm-run-all lint typecheck test",
    "verify:e2e": "playwright test --reporter=line --reporter=html",
    "verify:rc": "node scripts/ci/rc-verify.mjs"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@tanstack/react-query": "^5.40.1",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.12",
    "@tanstack/table-core": "^8.21.3",
    "archiver": "^6.0.2",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "express-rate-limit": "^7.3.1",
    "fast-json-stable-stringify": "^2.1.0",
    "helmet": "^7.1.0",
    "intl-tel-input": "^25.11.3",
    "lucide-react": "^0.446.0",
    "lucide-static": "^0.546.0",
    "nanoid": "^4.0.2",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "node-forge": "^1.3.1",
    "otplib": "^12.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.12",
    "react-router-dom": "^6.28.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "@cyclonedx/cyclonedx-npm": "^4.0.2",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "dotenv": "latest",
    "eslint": "^9.11.1",
    "husky": "^9.0.11",
    "jsdom": "^24.1.0",
    "license-checker-rseidelsohn": "^4.4.2",
    "npm-run-all": "latest",
    "openai": "latest",
    "postcss": "^8.4.41",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.10",
    "tsx": "^4.19.0",
    "vite": "^5.4.0",
    "vitest": "^2.0.5",
    "yaml": "^2.5.0",
    "@eslint/js": "^9.11.1",
    "@playwright/test": "^1.47.2",
    "globals": "^15.8.0",
    "lighthouse": "^12.1.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.9.0",
    "@types/testing-library__jest-dom": "^6.0.0"
  },
  "scriptsPrivate": {
    "audit": "node scripts/generate-audit.mjs"
  }
}

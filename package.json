{
  "name": "taxipartner-admin-suite",
  "version": "1.3.1097",
  "private": true,
  "type": "module",
  "scripts": {
    "identity:test": "npx tsx identity/tests/roleGuard.spec.ts && npx tsx identity/tests/permissionTemplates.spec.ts",
    "predev": "node scripts/strip-webkit-text-size-adjust.mjs || true",
    "dev": "vite",
    "verify:mp18": "node --loader tsx scripts/verify.mp18.mjs",
    "build": "vite build && tailwindcss -i src/styles/tailwind.css -o dist/output.css --minify && node scripts/strip-webkit-text-size-adjust.mjs || true",
    "preview": "vite preview",
    "build:css": "tailwindcss -i src/styles/tailwind.css -o dist/output.css --minify",
    "test": "npm run identity:test && vitest",
    "sandbox": "node scripts/sandbox-server.mjs",
    "lint": "eslint . --ext .js",
    "version:sync": "node scripts/version-sync.mjs",
    "auto-release:setup": "node scripts/auto-release-setup.mjs",
    "watch:auto-release": "bash scripts/local_watch.sh start",
    "watch:auto-release:stop": "bash scripts/local_watch.sh stop",
    "watch:auto-release:status": "bash scripts/local_watch.sh status",
    "changelog": "node scripts/generate-changelog.mjs",
    "validate:modules": "node scripts/validate-modules.mjs",
    "validate:releases": "node scripts/validate-release-log.mjs",
    "sbom": "cyclonedx-npm --json -o release-pack/sbom.json",
    "license:report": "license-checker-rseidelsohn --json --production --out release-pack/licenses.json",
    "release:pack": "node scripts/build-release-pack.mjs",
    "release:auto": "node scripts/auto-release-log.mjs",
    "prepare": "husky install",
    "postinstall": "node scripts/auto-release-setup.mjs || true",
    "typecheck": "tsc -p identity/tsconfig.json --noEmit",
    "check": "npm run typecheck",
    "build:qa": "npm run check && vite build",
    "qa:summary": "node --loader tsx scripts/route_map.mjs",
    "identity:dev": "npx tsx identity/src/server.ts",
    "identity:build": "tsc -p identity/tsconfig.json",
    "scheduler:dev": "npx tsx identity/src/tasks.scheduler.ts",
    "db:up": "docker compose up -d",
    "db:down": "docker compose down",
    "db:gen": "prisma generate",
    "db:migrate": "prisma migrate dev --name migrate_pg",
    "db:seed": "tsx prisma/seed.ts",
    "db:ensure": "prisma generate && prisma migrate dev --name mp18_sync",
    "seed:mp18": "tsx scripts/seed.mp18.ts",
    "db:backup": "node scripts/pg-backup.js",
    "db:restore": "node scripts/pg-restore.js",
    "db:rls:auto": "bash scripts/db/rls-auto-apply.sh",
    "tasks:smoke": "npx tsx identity/scripts/task.smoke.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "archiver": "^6.0.2",
    "cors": "^2.8.5",
    "express-rate-limit": "^7.3.1",
    "fast-json-stable-stringify": "^2.1.0",
    "helmet": "^7.1.0",
    "jose": "^5.10.0",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "node-forge": "^1.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "@cyclonedx/cyclonedx-npm": "^4.0.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.11.1",
    "husky": "^9.0.11",
    "jsdom": "^24.1.0",
    "license-checker-rseidelsohn": "^4.4.2",
    "postcss": "^8.4.41",
    "prisma": "^5.22.0",
    "tailwindcss": "^4.1.16",
    "vite": "^5.4.0",
    "vitest": "^2.0.5",
    "yaml": "^2.5.0",
    "tsx": "^4.19.0"
  },
  "scriptsPrivate": {
    "audit": "node scripts/generate-audit.mjs"
  }
}
